      "outputs": [],
      "source": [
        "import numpy as np\n",
        "\n",
        "X = 2 * np.random.rand(100, 1)\n",
        "y = 4 + 3 * X + np.random.randn(100, 1)"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "X_b = np.c_[np.ones((100, 1)), X]  # 모든 샘플에 x0 = 1을 추가합니다."
      ],
      "metadata": {
        "id": "b-6qL4i9OGMH"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "X_new = np.array([[0], [2]])\n",
        "X_new_b = np.c_[np.ones((2, 1)), X_new]  # 모든 샘플에 x0 = 1을 추가합니다."
      ],
      "metadata": {
        "id": "11V0-mxdOGOO"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "markdown",
      "source": [
        "# SGD 확률적 경사 하강법"
      ],
      "metadata": {
        "id": "fU5woxb0dA9b"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "theta_path_sgd = []\n",
        "m = len(X_b)\n",
        "np.random.seed(42)"
      ],
      "metadata": {
        "id": "zGpg_0rqN7iH"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "epochs = 50 # 에폭 수\n",
        "t0, t1 = 10, 100  # 학습 스케줄 하이퍼파라미터 (사용자가 수정 가능함)\n",
        "\n",
        "def lr_schedule(t): # 입력으로 들어오는 t는 점점 커지며 return값인 eta(학습률)은 점점 작아진다.\n",
        "    return t0 / (t + t1)\n",
        "\n",
        "theta = np.random.randn(2,1)  # 세타를 랜덤으로 설정함\n",
        "\n",
        "for epoch in range(epochs):\n",
        "    for i in range(m):                               # 하나의 에폭 당 100개의 랜덤 값을 뽑는다.\n",
        "        if epoch == 0 and i < 10:                    # 첫 에폭일 때 plot을 10번 그린다.\n",
        "            y_predict = X_new_b.dot(theta)           # 예측\n",
        "            style = \"b-\" if i > 0 else \"r--\"         # 첫번째 예측 값을 빨간선으로 그리고 그 이후는 파란색 선으로 그린다.\n",
        "            plt.plot(X_new, y_predict, style)\n",
        "        random_index = np.random.randint(m)          # m범위 안에서 랜덤값을 뽑기\n",
        "        xi = X_b[random_index:random_index+1]        # 입력 값\n",
        "        yi = y[random_index:random_index+1]          # 실제 데이터 값\n",
        "        gradients = 2 * xi.T.dot(xi.dot(theta) - yi) # 그래디언트 구하는 공식\n",
        "        eta = lr_schedule(epoch * m + i)       # 학습률 계산하기 (SGD이기 때문에 eta는 점점 작아진다)\n",
        "        theta = theta - eta * gradients              # 세타 계산하기\n",
        "        theta_path_sgd.append(theta)\n",
        "\n",
        "plt.plot(X, y, \"b.\")\n",
        "plt.xlabel(\"$x_1$\", fontsize=18)\n",
        "plt.ylabel(\"$y$\", rotation=0, fontsize=18)\n",
        "plt.axis([0, 2, 0, 15])\n",
        "save_fig(\"sgd_plot\")\n",
        "plt.show()"
      ],
      "metadata": {
